{
  "name": "angular-onsenui2-webpack2",
  "version": "1.0.0",
  "description": "Angular + Onsen UI 2 + Webpack2 + PhoneGap + TypeScript + PostCSS",
  "main": "src/js/app.js",
  "scripts": {
    "build": "webpack --progress --colors",
    "build:prod": "npm run clean && webpack --env=prod --progress --colors",
    "build:aot": "npm run clean && webpack --env=aot --progress --colors",
    "serve": "phonegap serve",
    "clean": "rimraf ./www/assets/*",
    "watch:serve": "npm run serve",
    "watch:webpack": "webpack --progress --colors --watch",
    "start": "npm-run-all -p watch:*",
    "test": "karma start ./config/karma.conf.js"
  },
  "keywords": [
    "Angular",
    "cssnext",
    "Onsen UI",
    "PhoneGap",
    "PostCSS",
    "SCSS",
    "TypeScript",
    "Webpack",
    "Yarn"
  ],
  "author": "puku0x",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^4.0.3",
    "@angular/compiler": "^4.0.3",
    "@angular/compiler-cli": "^4.0.3",
    "@angular/core": "^4.0.3",
    "@angular/forms": "^4.0.3",
    "@angular/http": "^4.0.3",
    "@angular/platform-browser": "^4.0.3",
    "@angular/platform-browser-dynamic": "^4.0.3",
    "@angular/router": "^4.0.3",
    "angular2-onsenui": "^1.0.0-rc.6",
    "core-js": "^2.4.1",
    "onsenui": "2.2.4",
    "rxjs": "^5.3.0",
    "zone.js": "0.8.7"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.3.0",
    "@types/core-js": "^0.9.41",
    "@types/jasmine": "^2.5.47",
    "@types/node": "^7.0.13",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.2",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.6.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.3",
    "npm-run-all": "^4.0.2",
    "null-loader": "^0.1.1",
    "path": "^0.12.7",
    "phonegap": "^6.5.0",
    "postcss": "^5.2.17",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-mixins": "^5.4.1",
    "postcss-nested": "^1.0.1",
    "postcss-simple-vars": "^3.1.0",
    "postcss-url": "^6.0.4",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "to-string-loader": "^1.1.5",
    "typescript": "^2.2.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1"
  }
}
